	[Symbol.match](+objects+)
	[Symbol.replace](+strings+)
	[Symbol.replace](+objects+)
	[Symbol.search](+strings+)
	[Symbol.search](+objects+)
	[Symbol.split](+strings+)
	[Symbol.split](+objects+)

RegExpStaticVariables :=
	RegExp.input
	RegExp.$%range%(1-9)
	RegExp.lastMatch
	RegExp.lastParen
	RegExp.leftContext
	RegExp.rightContext

