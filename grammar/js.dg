%%% ##############################################################################################

MapConstructorArguments :=
	[%repeat%(["+propertyNames+", +all+], ", ")]
	[%repeat%([+smallUnsigned+, +all+], ", ")]
	[%repeat%([+unsigned+, +all+], ", ")]

MapMethods :=
	+clear+
	+mapDelete+
	+entries+
	+mapForEach+
	+mapGet+
	+mapHas+
	+keys+
	+mapSet+
	+values+

MapAccessors :=
	size

clear :=
	clear()

mapDelete :=
	delete("+propertyNames+")

mapForEach :=
	forEach()
	forEach(+genericEmptyFunc+)
	forEach(+genericFuncOneArg+)
	forEach(+genericFuncTwoArgs+)

mapGet :=
	get("+propertyNames+")

mapHas :=
	has("+propertyNames+")

mapSet :=
	set("+propertyNames+", +all+)

